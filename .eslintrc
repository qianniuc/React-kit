{
  "globals": {},
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "react/prop-types": [2, { "ignore": ["children"] }],
    "react/prefer-stateless-function": 0,
    "react/jsx-indent": [2, 4],
    "jsx-a11y/img-has-alt": 0,
    "indent": [2, 4],
    "arrow-parens": [2, "as-needed"],
    "no-unused-vars": 0,
    "eol-last": 0,
    "no-plusplus": 0,
    "no-param-reassign": 0,
    "no-useless-escape": 0,
    "global-require": 0,
    "no-console": 0 //控制台打印信息不警告
  }
}